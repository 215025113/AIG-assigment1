using CSV, DataFrames

df = CSV.read("./bank-additional-full.csv")
dff=convert(Matrix,df)


#extract features
features = df[:, 11: 12]
target = df[:, end]



#renaming column

rename!(df,Dict(:y => :placed_deposit))

#clean data - string to numeric

#DATA CLEANING
size(features)[2]
for i = 1:(size(features)[2]-2)
    uniquee = unique(features[:,i])
    for j = 1:size(features)[1]
        for k = 1:size(uniquee)[1]
            if features[j,i] == uniquee[k]
                features[j,i] = Int(k)
               
            end
        end
    end
end
